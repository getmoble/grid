<h2>Manage Vendors</h2>
<vendor></vendor>

<script type="text/html" id="vendor-template">
    <div class="col-md-8">
        <vendor-list params="entity: 'vendor', enableFilter:true, manageList:true"></vendor-list>
    </div>
    <div class="col-md-4" data-bind="visible:showQuickView">
        <vendorupdate params="entity: 'vendor'"></vendorupdate>
    </div>
</script>
<script type="text/html" id="vendorsearch-template">
    <div class="col-sm-3">
        <label>Name</label>
        <input type="text" />
    </div>
    <div class="col-sm-3">
        <label></label>
        <button class="btn btn-primary" style="margin-top:25px;" data-bind="click:filter">Filter</button>
        <button class="btn btn-default" style="margin-top:25px;" data-bind="click:refresh">Reset</button>
    </div>
</script>

@section Scripts{
    <script>
        $(function () {
            ko.applyBindings();
        });
    </script>
}